(declare-fun PersonHasAgeCategory (String String) Bool)
(declare-fun Person (String Int String) Bool)
(declare-fun IsAlive (String) Bool)
(assert (forall ((p String) (age Int) (age_category String) (living_status String))
  (=> (and (Person p age living_status) (< 44 age) (> 65 age))
      (PersonHasAgeCategory p "MIDDLE_AGED"))))
(assert (forall ((p String) (age Int) (age_category String) (living_status String))
  (=> (and (Person p age living_status) (< 64 age))
      (PersonHasAgeCategory p "OLD"))))
(assert (forall ((p String) (age Int) (age_category String) (living_status String))
  (=> (and (Person p age living_status) (> 45 age))
      (PersonHasAgeCategory p "YOUNG"))))
(assert (forall ((p String) (age Int) (living_status String))
  (=> (Person p age "ALIVE") (IsAlive p))))
