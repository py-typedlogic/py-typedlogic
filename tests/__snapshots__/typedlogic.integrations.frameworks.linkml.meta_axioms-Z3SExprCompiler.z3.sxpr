(declare-fun Disjoint (String String) Bool)
(declare-fun Parent (String String) Bool)
(declare-fun InvalidClassSlot (String String) Bool)
(declare-fun ClassSlot (String String) Bool)
(declare-fun SlotDefinition (String) Bool)
(declare-fun ClassDefinition (String) Bool)
(declare-fun SlotUsage (String String) Bool)
(declare-fun Attribute (String String) Bool)
(declare-fun IsA (String String) Bool)
(declare-fun Mixin (String String) Bool)
(assert (forall ((lhs String) (rhs String) (parent String))
  (=> (and (Parent lhs parent) (Parent rhs parent) (distinct lhs rhs))
      (Disjoint lhs rhs))))
(assert (forall ((cls String) (slot String))
  (=> (and (ClassDefinition cls)
           (SlotDefinition slot)
           (not (ClassSlot cls slot)))
      (InvalidClassSlot cls slot))))
(assert (forall ((cls String) (slot_expression String))
  (=> (SlotUsage cls slot_expression) (Parent cls slot_expression))))
(assert (forall ((cls String) (slot_expression String))
  (=> (Attribute cls slot_expression) (Parent cls slot_expression))))
(assert (forall ((element String) (parent String))
  (=> (IsA element parent) (Parent element parent))))
(assert (forall ((element String) (parent String))
  (=> (Mixin element parent) (Parent element parent))))
(assert (forall ((cls String) (slot String) (parent String))
  (=> (and (Parent cls parent) (ClassSlot parent slot)) (Parent cls slot))))
