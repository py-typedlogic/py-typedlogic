(declare-fun Shaves (String String) Bool)
(declare-fun Person (String) Bool)
(declare-fun Barber (String) Bool)
(assert (forall ((shaver String) (customer String))
  (=> (and (Barber shaver) (Person customer) (not (Shaves customer customer)))
      (Shaves shaver customer))))
(assert (forall ((name String)) (=> (Barber name) (Person name))))
